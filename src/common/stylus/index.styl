 //头部样式
.header
    position:fixed
    height:50px
    text-align: center
    top:0
    left:0
    width:100%
    background: #ffffff;
    z-index: 9999;
    box-shadow: 0 1.5px 2px rgba(0, 0, 0, 0.2);
    .headerLogin
        position:absolute
        left:0
        top:0
        width:50px
        height:50px
        .noLogin
            i
                font-size:24px
                padding-top: 14px
                display: block
        .Login
            img
                height:30px
                width:30px
                vertical-align: top
                margin-top: 10px
    .headerTab
        border-radius: 2px
        border: 1px solid $color-background-origin
        box-sizing: border-box
        -webkit-box-sizing: border-box
        margin: 10px auto 0
        display: inline-block;
        a
            position: relative;
            font-size: 14px;
            color: $color-background-origin;
            letter-spacing: 0.82px;
            line-height: 28px;
            text-align: center;
            box-sizing: border-box;
            display: inline-block;
            &.active
                background:$color-background-origin;
                color:#ffffff
    .headerTabTwo
        a
            width:100px
    .headerTabThree
        width:70%
        a
            width:33.33%
    h2
        line-height:50px;
        font-size: 18px;
        color: #000;
    
.shadow 
    box-shadow: 0 1.5px 1.5px rgba(0, 0, 0, 0.2)
    border-radius: 3px
    background: #ffffff
    margin-bottom: 10px
    overflow:hidden

